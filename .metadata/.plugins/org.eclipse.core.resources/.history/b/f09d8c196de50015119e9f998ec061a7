#include <iostream>
#include <cstring>

using namespace std;

int main(int argc, char* argv[]) {
	cout << "Enter a double: ";
	double input;
	cin >> input;

	unsigned long number;
	memcopy(&number, &input, sizeof(number));

	return 0;
}
