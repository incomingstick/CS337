#include "intVector.h"

intVector::intVector(int size = 0)
	:count{ count = size }
{
	vector = new int[count];
	for (int i = 0; i < count; i++)
	        vector[i] = 0; //init vector elements to 0
}

intVector::~intVector() {
	// TODO Auto-generated destructor stub
	delete vector;
}

