#include <sys/stat.h>
#include <pwd.h>
#include <dirent.h>
#include <stdio.h>

using namespace std;

int main(int argc, char* argv[]) {
	struct stat fileInfo;
	if(stat(argv[1], &fileInfo) == -1)
		printf("Error: File not found.");
	return 0;
}
